{
	"manifest_version": 2,
	"name": "SFAA Extensions",
	"version": "1.0.0.8",
	"content_scripts": [
	{
		"matches": ["https://quad.my.salesforce.com/*", "https://*.visual.force.com/*"],
		"js": ["baseExtension.js", "workView.js", "buildExtension.js", "sprintExtension.js"]
	}
	],
	"options_ui": {
		"page": "options/index.html",
		"chrome_style": true
	},
	"permissions": [
		"storage"
	]
}